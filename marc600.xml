    <marc:datafield tag="300" ind1=" " ind2=" ">
        <marc:subfield code="a">1 atlas ([1] leaf, [71] leaves of plates) :</marc:subfield>
        <marc:subfield code="b">35 maps, port. ;</marc:subfield>
